{
  "name": "simple-shop-web",
  "version": "1.0.0",
  "description": "A simple shop website",
  "main": "index.mjs",
  "scripts": {
    "start": "pm2 start ecosystem.config.cjs --no-daemon",
    "stop": "pm2 kill",
    "dev": "cross-env NODE_ENV=development nodemon index.mjs",
    "test": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest",
    "test:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --watchAll ",
    "test:mix": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules --testPathIgnorePatterns=/tests/unit/ jest",
    "test:mix:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules --testPathIgnorePatterns=/tests/unit/ jest --watchAll",
    "test:unit": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/unit/",
    "test:unit:watch": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest tests/unit/ --watchAll",
    "coverage": "cross-env NODE_ENV=test NODE_OPTIONS=--experimental-vm-modules jest --coverage",
    "lint": "eslint **/*.mjs"
  },
  "type": "module",
  "keywords": [
    "restapi",
    "nodejs",
    "expressjs",
    "backend"
  ],
  "author": "David Dzung Dang",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ddbdzung/Simple-Shop-Web.git"
  },
  "license": "ISC",
  "dependencies": {
    "@sendgrid/mail": "^7.7.0",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "express": "^4.18.1",
    "express-mongo-sanitize": "^2.2.0",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "http-status": "^1.5.3",
    "joi": "^17.6.2",
    "jsonwebtoken": "^8.5.1",
    "luxon": "^3.0.4",
    "moment": "^2.29.4",
    "mongoose": "^6.6.4",
    "morgan": "^1.10.0",
    "nodemailer": "^6.8.0",
    "pm2": "^5.2.0",
    "rotating-file-stream": "^3.0.4",
    "slugify": "^1.6.5",
    "swagger-jsdoc": "^6.2.5",
    "swagger-ui-express": "^4.5.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@faker-js/faker": "^7.5.0",
    "@types/jest": "^29.1.2",
    "cross-env": "^7.0.3",
    "dotenv": "^16.0.3",
    "eslint": "^8.24.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-plugin-import": "2.25.2",
    "eslint-plugin-jest": "^27.1.1",
    "eslint-plugin-security": "^1.5.0",
    "jest": "^29.1.2",
    "node-mocks-http": "^1.11.0",
    "nodemon": "^2.0.20",
    "supertest": "^6.3.0"
  },
  "pnpm": {
    "peerDependencyRules": {
      "ignoreMissing": [
        "typescript",
        "openapi-types"
      ]
    }
  }
}
